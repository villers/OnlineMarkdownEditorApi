{
  "name": "MarkdownApi",
  "version": "1.0.0",
  "author": "Mickael VILLERS <mvillers@cegid.fr>",
  "private": "true",
  "description": "Server for convert MArkdown to PDF",
  "dependencies": {
    "body-parser": "^1.15.0",
    "copy-webpack-plugin": "^1.1.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "express-decorators": "^0.3.0",
    "file-loader": "^0.8.5",
    "highlight.js": "^9.1.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "markdown-it": "^5.1.0",
    "markdown-it-abbr": "^1.0.3",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-deflist": "^2.0.0",
    "markdown-it-emoji": "^1.1.0",
    "markdown-it-footnote": "^2.0.0",
    "markdown-it-headinganchor": "^1.3.0",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc": "^1.1.0",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "stylus-loader": "^1.5.1",
    "system": "^1.0.4",
    "ts-loader": "^0.8.1",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "webpack": "^1.12.13",
    "webpage": "^0.3.0"
  },
  "scripts": {
    "postinstall": "npm run typings-install && npm run build",
    "build": "node ./node_modules/webpack/bin/webpack --colors --progress --display-error-details -p --config webpack.server.config.js",
    "prestart": "npm run build",
    "start": "cd server && node server.js",
    "clean": "node ./node_modules/rimraf/bin server target Logs",
    "docs": "node ./node_modules/typedoc/bin/typedoc --options typedoc.json --includeDeclarations typings/browser.d.ts src",
    "typings-install": "typings install",
    "typings-install-e": "typings install --ambient --save"
  }
}
